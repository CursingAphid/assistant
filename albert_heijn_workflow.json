      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [240, 300],
      "webhookId": "ah-price-check"
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "product_name",
              "value": "={{ $json.product_name || $json.query || 'milk' }}"
