            },
            {
              "name": "url",
              "value": "={{ $json.url || 'Not available' }}"
            },
            {
              "name": "availability",
              "value": "={{ $json.availability || 'Unknown' }}"
            },
            {
              "name": "execution_time",
              "value": "={{ $json.execution_time }}"
            }
          ]
        },
        "options": {}
      },
      "id": "format-success-response",
