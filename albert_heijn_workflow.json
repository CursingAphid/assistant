              "name": "timestamp",
              "value": "={{ new Date().toISOString() }}"
            }
          ]
        },
        "options": {}
      },
      "id": "set-product-name",
      "name": "Set Product Name",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [460, 300]
    },
    {
      "parameters": {
        "command": "cd /Users/milanalbertz/Desktop/projects/n8n && python3 ah_price_checker_demo.py \"={{ $json.product_name }}\"",
        "options": {}
      },
      "id": "execute-python-script",
      "name": "Execute Python Script",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [680, 300]
    },
    {
      "parameters": {
        "jsCode": "// Parse the Python script output\nconst output = $input.first().json.stdout;\nlet result;\n\ntry {\n  result = JSON.parse(output);\n} catch (error) {\n  result = {\n    success: false,\n    error: 'Failed to parse Python output',\n    raw_output: output\n  };\n}\n\n// Add additional metadata\nresult.execution_time = new Date().toISOString();\nresult.n8n_execution_id = $execution.id;\n\nreturn { json: result };"
      },
      "id": "parse-python-output",
      "name": "Parse Python Output",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [900, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "condition-success",
              "leftValue": "={{ $json.success }}",
              "rightValue": true,
              "operator": {
                "type": "boolean",
                "operation": "equal"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-success",
      "name": "Check Success",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [1120, 300]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "status",
              "value": "success"
            },
            {
              "name": "message",
              "value": "Price found successfully"
            },
            {
              "name": "product_name",
              "value": "={{ $json.product_name }}"
            },
            {
              "name": "found_product_name",
              "value": "={{ $json.found_product_name || $json.product_name }}"
            },
            {
              "name": "price",
              "value": "={{ $json.price || 'Not available' }}"
            },
            {
              "name": "unit_price",
              "value": "={{ $json.unit_price || 'Not available' }}"
            },
            {
              "name": "discount",
